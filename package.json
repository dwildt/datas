{
  "name": "datas-utils",
  "version": "1.0.0",
  "description": "Utilitários JavaScript para manipulação de datas",
  "main": "date-utils.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["dates", "calendar", "moon", "utilities", "javascript"],
  "author": "Wildtech",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "collectCoverageFrom": [
      "*.js",
      "!jest.config.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/*.test.js"
    ]
  }
}